<h1>Editing Course #<%= @course.id %></h1>

<form action="/courses/<%= @course.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @course.name  %>" type="text" />
  </div>
  <div>
    <label for="description">description</label><br />
    <input id="description" name="description" value="<%= @course.description  %>" type="text" />
  </div>
  <div>
    <label for="teacher_id">teacher_id</label><br />
    <input id="teacher_id" name="teacher_id" value="<%= @course.teacher_id  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>



<h1>Editing Teacher #<%= @teacher.id %></h1>

<form action="/teachers/<%= @teacher.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @teacher.name  %>" type="text" />
  </div>
  <div>
    <label for="email">email</label><br />
    <input id="email" name="email" value="<%= @teacher.email  %>" type="text" />
  </div>
  <div>
    <label for="department">department</label><br />
    <input id="department" name="department" value="<%= @teacher.department  %>" type="text" />
  </div>
  <div>
    <label for="bio">bio</label><br />
    <input id="bio" name="bio" value="<%= @teacher.bio  %>" type="text" />
  </div>
  <div>
    <label for="image_url">image_url</label><br />
    <input id="image_url" name="image_url" value="<%= @teacher.image_url  %>" type="text" />
  </div>
  <div>
    <label for="location">location</label><br />
    <input id="location" name="location" value="<%= @teacher.location  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>



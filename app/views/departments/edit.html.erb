<h1>Editing Department #<%= @department.id %></h1>

<form action="/departments/<%= @department.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @department.name  %>" type="text" />
  </div>
  <div>
    <label for="teacher_id">teacher_id</label><br />
    <input id="teacher_id" name="teacher_id" value="<%= @department.teacher_id  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


